<p id="notice"><%= notice %></p>

<h1>Wants</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wants.each do |want| %>
      <tr>
        <td><%= want.user_id %></td>
        <td><%= want.about %></td>
        <td><%= link_to 'Show', want %></td>
        <td><%= link_to 'Edit', edit_want_path(want) %></td>
        <td><%= link_to 'Destroy', want, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Want', new_want_path %>
